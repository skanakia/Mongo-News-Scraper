<div class="col-md-12">
    {{#if articles}} {{#each articles}}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4>{{this.title}}</h4>
        </div>
        <div class="panel-body">
            <p> {{this.summary}}</p>
            <a href={{this.link}} target="_blank">{{this.link}}</a>
            <br>
            <br>
            <button type="button" class="btn btn-success save" data-id={{this._id}}>Save Article</button>
        </div>
    </div>
    {{/each}} {{else}}
    <div class="panel panel-default">
        <div class="panel-body">
            <h4>Sorry, No new articles. Please try again later.</h4>
        </div>
    </div>
    {{/if}}
</div>
 <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
<script src="./js/home.js"></script>